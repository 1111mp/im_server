{
  "name": "im_server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development LOAD_CONFIG=dev TS_NODE_FILES=true ts-node --project tsconfig.json -r @noshot/env -r tsconfig-paths/register run/index.ts",
    "start:watch": "nodemon --watch **/* -e ts,js,json --exec cross-env NODE_ENV=development LOAD_CONFIG=dev TS_NODE_FILES=true ts-node --project tsconfig.json -r @noshot/env -r tsconfig-paths/register run/index.ts",
    "prod": "cross-env NODE_ENV=production LOAD_CONFIG=dev TS_NODE_FILES=true pm2 start pm2.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "proto": "npx pbjs -t static-module -w commonjs -o proto/proto.js proto/*.proto && pbts -o proto/proto.d.ts proto/proto.js"
  },
